/* {{ warning }} */

* {
    color-foreground: {{ colors.rofi.foreground }};
    color-background: {{ colors.rofi.background }};

    color-black: {{ colors.normal.black }};
    color-red: {{ colors.normal.red }};
    color-green: {{ colors.normal.green }};
    color-yellow: {{ colors.normal.yellow }};
    color-blue: {{ colors.normal.blue }};
    color-magenta: {{ colors.normal.magenta }};
    color-cyan: {{ colors.normal.cyan }};
    color-white: {{ colors.normal.white }};

    color-black-bright: {{ colors.bright.black }};
    color-red-bright: {{ colors.bright.red }};
    color-green-bright: {{ colors.bright.green }};
    color-yellow-bright: {{ colors.bright.yellow }};
    color-blue-bright: {{ colors.bright.blue }};
    color-magenta-bright: {{ colors.bright.magenta }};
    color-cyan-bright: {{ colors.bright.cyan }};
    color-white-bright: {{ colors.bright.white }};

    color-foreground-light: {{ lighten colors.primary.foreground 5 }}; /* TODO(SuperPaintman): move it into config. */
    color-background-light: {{ lighten colors.primary.background 5 }}; /* TODO(SuperPaintman): move it into config. */
}
